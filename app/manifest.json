{
  "name": "Client Health Check Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Checks a bunch of stuff on the client",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/q.js",
      "scripts/chromereload.js",
      "scripts/background.js",
      "scripts/jquery.min.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///home/sajit/Amplify/hackathons/my-new-chrome-extension/sample/sample.html"

      ],
      "js": [
       "scripts/detect.min.js", "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab"
  ],
  "browser_action" : {
      "default_icon" : "icon.png"
  }
}
